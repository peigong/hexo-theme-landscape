<% if (site.data.links && site.data.links.length){ %>
  <div class="widget-wrap">
    <h3 class="widget-title">
      <img src="/images/weixin.png" alt="weixin" width="100%" />
    </h3>
    <div class="widget">
      <ul>
        <% site.data.links.forEach(function(link){ %>
          <li>
            <a href="<%- url_for(link.url) %>"><%= link.title %></a>
          </li>
        <% }) %>
      </ul>
    </div>
  </div>
<% } %>